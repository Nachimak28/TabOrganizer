{
    "manifest_version": 3,
    "name": "Tab Organizer",
    "version": "1.0",
    "description": "Export URLs and group names of all tabs in the current window to a CSV file.",
    "permissions": ["tabs", "tabGroups", "downloads"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "128x128.png"
    },
    "icons": {
      "16": "16x16.png",
      "48": "48x48.png",
      "128": "128x128.png"
    }
  }
  